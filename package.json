{
  "name": "gaelic-cup-planner",
  "version": "1.0.0",
  "description": "Simplify planning of gaelic games tournaments",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "help": "clear;cat docs/developers-guide.md",
    "start": "bash scripts/build-fe.sh --watch",
    "launch": "bash scripts/launch.sh",
    "sync": "node src/backend/server.js --sync-from-master=true",
    "deploy": "bash scripts/deploy.sh",
    "build": "bash scripts/build-fe.sh --build",
    "db:setup": "bash scripts/db-setup.sh",
    "test:fe": "bash scripts/test-fe.sh",
    "relink:css": "bash node_modules/gather-links/bin/relink.sh --type=css --type=scss --src=. --dest=../all-styles --ignore=node_modules",
    "pstart": "parcel src/frontend/interfaces/player/index.html --dist-dir ./src/apps/player",
    "pbuild": "parcel build src/frontend/interfaces/player/index.html --dist-dir ./src/apps/player",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "ws:_init": "wsswitch --init",
    "ws": "wsswitch --select"
  },
  "parcel": {
    "aliases": {
      "$shared": "src/frontend/shared/css/"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@parcel/transformer-inline-string": "^2.10.1",
    "@parcel/transformer-sass": "^2.10.3",
    "@storybook/addon-actions": "^7.6.12",
    "@storybook/addon-essentials": "^7.6.5",
    "@storybook/addon-interactions": "^7.6.5",
    "@storybook/addon-links": "^7.6.5",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addon-viewport": "^7.6.17",
    "@storybook/blocks": "^7.6.5",
    "@storybook/react": "^7.6.5",
    "@storybook/react-vite": "^7.6.5",
    "@storybook/react-webpack5": "^7.6.5",
    "@storybook/test": "^7.6.5",
    "babel-cli": "^6.26.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "node-sass": "^9.0.0",
    "parcel": "^2.10.3",
    "postcss": "^8.4.35",
    "postcss-modules": "^4.3.1",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react-dev-utils": "^12.0.1",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "storybook": "^7.6.5",
    "style-loader": "^3.3.3"
  },
  "dependencies": {
    "@types/google-apps-script": "^1.0.76",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "gather-links": "^1.0.0",
    "googleapis": "^128.0.0",
    "mysql": "^2.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0"
  }
}
